<link href="/css/love.css" rel="stylesheet" media="all">
<link href="/css/result.css" rel="stylesheet" media="all">
<div class="page" ontouchstart="">
  <div class="firstContents">
    <div class="image-container">
      <div class="image">
        <div class="face-out-circle">
          <img src="<%= @target_img %>">
        </div>
      </div>
    </div>
    <div class="borderCircle circle1"></div>
    <div class="borderCircle circle2"></div>
    <div class="circle circle3"></div>
    <div class="circle circle4"></div>
    <div class="profile">
      <div class="name"><%= @target_name %></div>
      <div class="email"><%= @target_email %></div>
    </div>
  </div>
  <div class="secondContents">

    <div class="cover">
      <div class="line-out">
        <div class="line-in">
          <p>
            告白成功！
          </p>
        </div>
      </div>
    </div>

  </div>
</div>
<script type="text/javascript" src="js/main.js"></script>
